        return null;
    }
    
    public boolean LectureNoteBean.isCreateDialogVisible() {
        return createDialogVisible;
    }
    
    public void LectureNoteBean.setCreateDialogVisible(boolean createDialogVisible) {
        this.createDialogVisible = createDialogVisible;
    }
    
    public String LectureNoteBean.displayList() {
        createDialogVisible = false;
        findAllLectureNotes();
        return "lectureNote";
    }
    
    public String LectureNoteBean.displayCreateDialog() {
        lectureNote = new LectureNote();
        createDialogVisible = true;
        return "lectureNote";
    }
    
    public String LectureNoteBean.persist() {
        String message = "";
        if (lectureNote.getId() != null) {
            lectureNote.merge();
            message = "Successfully updated";
        } else {
            lectureNote.persist();
            message = "Successfully created";
        }
        RequestContext context = RequestContext.getCurrentInstance();
        context.execute("createDialog.hide()");
        context.execute("editDialog.hide()");
        
        FacesMessage facesMessage = new FacesMessage(message);
        FacesContext.getCurrentInstance().addMessage(null, facesMessage);
        reset();
        return findAllLectureNotes();
    }
    
    public String LectureNoteBean.delete() {
        lectureNote.remove();
        FacesMessage facesMessage = new FacesMessage("Successfully deleted");
        FacesContext.getCurrentInstance().addMessage(null, facesMessage);
        reset();
        return findAllLectureNotes();
    }
    
    public void LectureNoteBean.reset() {
        lectureNote = null;
        createDialogVisible = false;
    }
    
    public void LectureNoteBean.handleDialogClose(CloseEvent event) {
        reset();
    }
    
