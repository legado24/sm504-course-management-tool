        HtmlOutputText courseLabel = (HtmlOutputText) application.createComponent(HtmlOutputText.COMPONENT_TYPE);
        courseLabel.setId("courseLabel");
        courseLabel.setValue("Course:   ");
        htmlPanelGrid.getChildren().add(courseLabel);
        
        HtmlOutputText courseValue = (HtmlOutputText) application.createComponent(HtmlOutputText.COMPONENT_TYPE);
        courseValue.setValueExpression("value", expressionFactory.createValueExpression(elContext, "#{lectureNoteBean.lectureNote.course}", Course.class));
        courseValue.setConverter(new CourseConverter());
        htmlPanelGrid.getChildren().add(courseValue);
        
        HtmlOutputText dataLabel = (HtmlOutputText) application.createComponent(HtmlOutputText.COMPONENT_TYPE);
        dataLabel.setId("dataLabel");
        dataLabel.setValue("Data:   ");
        htmlPanelGrid.getChildren().add(dataLabel);
        
        UIComponent dataValue;
        if (lectureNote != null && lectureNote.getData() != null && lectureNote.getData().length > 0) {
            dataValue = (CommandButton) application.createComponent(CommandButton.COMPONENT_TYPE);
            ((CommandButton) dataValue).addActionListener(new FileDownloadActionListener(expressionFactory.createValueExpression(elContext, "#{lectureNoteBean.dataStreamedContent}", StreamedContent.class), null));
            ((CommandButton) dataValue).setValue("Download");
            ((CommandButton) dataValue).setAjax(false);
        } else {
            dataValue = (HtmlOutputText) application.createComponent(HtmlOutputText.COMPONENT_TYPE);
            ((HtmlOutputText) dataValue).setValue("");
        }
        htmlPanelGrid.getChildren().add(dataValue);
        
        return htmlPanelGrid;
    }
    
